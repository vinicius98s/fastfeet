{
  "name": "@fastfeet/server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "node ./dist/server.js",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "db:create": "createdb fastfeet --user=postgres",
    "db:migrate": "yarn sequelize db:migrate",
    "db:seed": "yarn sequelize db:seed:all",
    "db:up": "yarn db:create && yarn db:migrate && yarn db:seed",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test yarn pretest && jest --watch",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.30",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
